{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "type": "text",
          "template": ""
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#101010",
          "style": "plain",
          "template": "\ue0b6",
          "type": "text"
        },
        {
          "background": "#101010",
          "foreground": "#4696ff",
          "properties": {
            "home_icon": " \uE5FF ~",
            "style": "full"
          },
          "style": "diamond",
          "template": "{{ .Path }} <#8e7cc3>\uF105</>",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#101010",
          "foreground": "#d650ff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .RepoName}} \uE725 {{ .HEAD }} {{ .BranchStatus }} {{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}\uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#101010",
          "foreground": "#0dda85",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ece63d{{ end }}",
            "{{ if gt .Ahead 0 }}#92da0d{{ end }}",
            "{{ if gt .Behind 0 }}#c53379{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_max_length": 25
          },
          "style": "powerline",
          "template": "\uF105 <,transparent>\ue0b0</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "â€„",
          "type": "text"
        },
        {
          "background": "#101010",
          "foreground": "#51cbfb",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7",
          "type": "root"
        },
        {
          "background": "#101010",
          "foreground": "#00ff95",
          "foreground_templates": ["{{ if gt .Code 0 }}#a30000{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "leading_diamond": "",
          "style": "diamond",
          "template": " <#91e0ff>\u276F</> {{ if gt .Code 0 }} \uf00d {{.Code}} {{.Meaning}} {{ else }} \uf00c {{ end }}",
          "trailing_diamond": "\ue0b0",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{if .Root}}Admin: {{end}} {{.Folder}}",
  "final_space": true,
  "version": 2
}
